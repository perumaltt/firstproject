

<%= form_for :user, url: "/create", method: "post" do |f| %>
    <h4>Name:</h4>
    <%= f.text_field :name %>
    <h4>Phone</h4>
    <%= f.text_field :phone %><br>
    <h4>Email</h4>
    <%= f.text_field :email %><br>
    <h4>Address</h4>
    <%= f.text_field :address %><br>
    <br>
    <%= f.submit "submit" %>
  <% end %>